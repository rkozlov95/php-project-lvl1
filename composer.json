{
    "name": "rkozlov95/php-brain-games",
    "bin": ["bin/brain-even"],
    "description": "Simply CLI games",
    "scripts": {
      "brain-even": ["bin/brain-even"],
      "phpcs": "phpcs"
    },
    "autoload": {
      "files": [
            "src/Engine.php",
            "src/games/Even.php"
      ],
      "psr-4": {
        "BrainGames\\": "src/",
        "BrainGames\\games\\": "src/games"
      }
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Kozlov",
            "email": "rkozlov95@mail.ru"
        }
    ],
    "require": {
        "wp-cli/php-cli-tools": "^0.11.11"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
